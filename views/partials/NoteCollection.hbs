{{#unless noteList}}
  <p class="ui container center large  aligned message">{{fullname}} Looking very quiet in here add a Note List</p>
{{/unless}}



{{#each noteList}}

  <div class="ui inverted icon message">
    <i class="{{iconName}} icon"></i>
    <div class="content">
      <div class="header">
        <h1>
          {{title}}
        </h1>
      </div>
      <div class="catergory">
        <p>
          Catergory:
          {{catergory}}
        </p>
      </div>
      <div class="Description ui">
        <p>
          Description:
          {{description}}
        </p></div>

      <p>
        Priority:
        <span id="n{{id}}"></span>
      </p>

    </div>
    <a class="ui small grey button" href="/notelist/{{id}}">View</a>
    <a
      class="ui small white inverted button delnotecollection"
      href="/dashboard/deleteNoteCollection/{{id}}"
    >
      <i class="ui trash icon"></i></a>

  </div>
  <script>
    for (let i=0; i <
    {{priorty}}; i++) { document.querySelector("#n{{id}}").innerHTML += "<i class='bolt icon'></i>";}
  </script>

{{/each}}