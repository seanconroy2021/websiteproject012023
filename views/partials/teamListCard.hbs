<div class="ui card inverted" id ="{{name}}">
  
  <div class="ui circular image">
    <img src="{{teamImg}}">
  </div>
  <div class="content">
    <a class="header">{{fullname}}</a>
    <div class="meta">
      <span class="date"> {{yearsSinceJoining joined}} year's with us.</span>
    </div>
    <div class="description">
      {{name}} is {{job}} in {{city}}
    </div>
  </div>
 
</div>